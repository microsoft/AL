{
    "Snippet: Table": {
        "prefix": "ttable",
        "body": [
            "table ${1:id} ${2:MyTable}",
            "{",
            "",
            "\tfields",
            "\t{",
            "\t\tfield(1;${3:MyField};${4:Integer})",
            "\t\t{",
            "\t\t}",
            "\t}",
            "",
            "\tkeys",
            "\t{",
            "\t\tkey(${5:PK};${3:MyField})",
            "\t\t{",
            "\t\t\tClustered = true;",
            "\t\t}",
            "\t}",
            "\t",
            "\tvar",
            "\t\t${8:myInt : Integer;}",
            "",
            "\ttrigger OnInsert();",
            "\tbegin",
            "\tend;",
            "",
            "\ttrigger OnModify();",
            "\tbegin",
            "\tend;",
            "",
            "\ttrigger OnDelete();",
            "\tbegin",
            "\tend;",
            "",
            "\ttrigger OnRename();",
            "\tbegin",
            "\tend;",
            "",
            "}"
        ],
        "description": "Snippet: Table"
    },
